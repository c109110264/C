#include <stdio.h>
#include <stdlib.h>
#include <time.h>
/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char *argv[]) {
int a,i,m[6];
scanf("%d %d %d %d %d %d %d ",&a,&m);
srand(time(NULL));

for(i=0;i<6;i++)
	m[i]=rand( )%49+1;
for(i=0;i<6;i++)
printf("%d %d\n",a,m);

if(a==m)
printf("\n答對\n");
else
printf("\n答錯\n");



	return 0;
}
